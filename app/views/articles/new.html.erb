<%= render partial: 'shared/article_actions' %>

<div class="placeIt">
	<h1> New Article </h1>		
	<%= form_for @article do |f| %>
		<div class="form-group col-xs-4">
			<%= render partial: 'shared/form', locals: {f: f} %>
			
			<br />

			<%= f.submit "Create Article", class: "btn btn-success" %>
		</div>	
	<% end %>
</div>

